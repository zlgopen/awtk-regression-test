<window text="Image">
  <list_view x="0"  y="0" w="100%" h="-100" item_height="128" auto_hide_scroll_bar="true">
    <scroll_view name="view" x="0"  y="0" w="-12" h="100%">
     <list_item style="empty" children_layout="default(r=1,c=3,s=2,m=2)">
      <image name="image1" clickable="true" image="1" draw_type="scale_auto"><label w="100%" h="100%" text="clickable" /></image>
      <image name="image2" selectable="true" style="selectable" image="2" draw_type="scale_auto"><label w="100%" h="100%" text="selectable" /></image>
      <image name="image3" clickable="true" selectable="true" style="selectable" image="3" draw_type="scale_auto"><label w="100%" h="100%" text="auto" /></image>
     </list_item> 
     <list_item style="empty" children_layout="default(r=1,c=3,s=2,m=2)">
      <image style="selectable" selectable="true"image="earth" draw_type="default"><label w="100%" h="100%" text="default" /></image>
      <image style="selectable" selectable="true"image="earth" draw_type="icon"><label w="100%" h="100%" text="icon" /></image>
      <image style="selectable" selectable="true"image="earth" draw_type="center"><label w="100%" h="100%" text="center" /></image>
     </list_item>
     <list_item style="empty" children_layout="default(r=1,c=3)">
      <image style="selectable" selectable="true"image="1" draw_type="scale_w"><label w="100%" h="100%" text="scale_w" /></image>
      <image style="selectable" selectable="true"image="1" draw_type="scale_h"><label w="100%" h="100%" text="scale_h" /></image>
      <image style="selectable" selectable="true"image="1" draw_type="scale"><label w="100%" h="100%" text="scale" /></image>
     </list_item>
     <list_item style="empty" children_layout="default(r=1,c=2,s=2,m=2)">
      <image style="selectable" selectable="true"image="bricks" draw_type="repeat"><label w="100%" h="100%" text="repeat" /></image>
      <image style="selectable" selectable="true"image="bricks" draw_type="repeat_x"><label w="100%" h="100%" text="repeat_x" /></image>
     </list_item>
     <list_item style="empty" children_layout="default(r=1,c=2,s=2,m=2)">
      <image style="selectable" selectable="true"image="bricks" draw_type="repeat_y"><label w="100%" h="100%" text="repeat_y" /></image>
      <image style="selectable" selectable="true"image="bricks" draw_type="repeat_y_inverse"><label w="100%" h="100%" text="repeat_y_inverse" /></image>
     </list_item>
     <list_item style="empty" children_layout="default(r=1,c=3,s=2,m=2)">
      <image style="selectable" selectable="true"image="green_btn_n" draw_type="patch9"><label w="100%" h="100%" text="patch9" /></image>
      <image style="selectable" selectable="true"image="green_btn_n" draw_type="patch3_x"><label w="100%" h="100%" text="patch3_x" /></image>
      <image style="selectable" selectable="true"image="green_btn_n" draw_type="patch3_y"><label w="100%" h="100%" text="patch3_y" /></image>
     </list_item>
     <list_item style="empty" children_layout="default(r=1,c=3,s=2,m=2)">
      <image style="selectable" selectable="true"image="green_btn_n" draw_type="repeat9"><label w="100%" h="100%" text="repeat9" /></image>
      <image style="selectable" selectable="true"image="green_btn_n" draw_type="repeat3_x"><label w="100%" h="100%" text="repeat3_x" /></image>
      <image style="selectable" selectable="true"image="green_btn_n" draw_type="repeat3_y"><label w="100%" h="100%" text="repeat3_y" /></image>
     </list_item>
     <list_item style="empty" children_layout="default(r=1,c=1)">
      <image style="selectable" selectable="true"image="green_btn_n" draw_type="patch3_x_scale_y"><label w="100%" h="100%" text="patch3_x_scale_y" /></image>
     </list_item>
     <list_item style="empty" children_layout="default(r=1,c=10)">
      <label/>
      <label/>
      <label/>
      <label/>
      <image style="selectable" selectable="true"image="green_btn_n" draw_type="patch3_y_scale_x"><label w="100%" h="100%" text="patch3_y_scale_x" /></image>
     </list_item>
    </scroll_view>
    <scroll_bar_m name="bar" x="right" y="0" w="6" h="100%" value="0"/>
  </list_view>

  <label name="status" x="c" y="bottom:80" w="40%" h="22" text="status"/>
  <button name="unload_unused" x="10" y="bottom:40" w="40%" h="30" text="Unload Unused"/>
  <button name="unload_all" x="r:10" y="bottom:40" w="40%" h="30" text="Unload All"/>
  <button name="other_test" x="10" y="bottom:5" w="40%" h="30" text="Other Test"/>
  <button name="close" x="r:10" y="bottom:5" w="40%" h="30" text="Close" on:click="back()"/>
</window>
