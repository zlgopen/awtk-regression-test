<window text="Fullscreen">
  <button name="fullscreen" focusable="true" focused="true" x="c" y="m:-50" w="128" h="30" text="fullscreen"/>
  <button name="unfullscreen" focusable="true" x="c" y="m" w="128" h="30" text="unfullscreen"/>

  <label name="status" x="c" y="b:50" w="128" h="30" text="none"/>

  <button focusable="true" x="c" y="b:10" w="128" h="30" text="close" on:click="back()"/>
</window>
