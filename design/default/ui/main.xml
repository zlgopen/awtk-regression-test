<window closable="no" anim_hint="htranslate" text="Regression Test">
  <list_view x="0"  y="0" w="100%" h="-50" item_height="36">
    <scroll_view name="view" x="0"  y="0" w="-12" h="100%" children_layout="list_view(x_margin=5,spacing=10)">
      <list_item style="even_clickable" name="auto_adjust_size" text="Test Auto Adjust Resize" /> 
      <list_item style="odd_clickable" name="auto_adjust_size_max_w" text="Test Auto Adjust Resize Max W" /> 
      <list_item style="even_clickable" name="auto_adjust_size_margin" text="Test Auto Adjust Resize Margin" /> 
      <list_item style="odd_clickable" name="rotation" text="Test Rotation" /> 
      <list_item style="even_clickable" name="dialogs" text="Test Dialogs" /> 
      <list_item style="odd_clickable" name="combo_box" text="Test ComboBox" /> 
      <list_item style="even_clickable" name="button" text="Test Buttons" /> 
      <list_item style="odd_clickable" name="check_button" text="Test CheckButton" /> 
      <list_item style="even_clickable" name="popup" text="Test Popup" /> 
    </scroll_view>
    <scroll_bar_d name="bar" x="right" y="0" w="12" h="100%" value="0"/>
  </list_view>
  <button name="close" x="center" y="bottom:10" w="50%" h="30" text="close" on:click="quit()"/>
</window>
