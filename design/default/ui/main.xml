<window closable="no" anim_hint="htranslate" text="Regression Test">
  <list_view x="0"  y="0" w="100%" h="-50" item_height="36">
    <scroll_view name="view" x="0"  y="0" w="-12" h="100%" children_layout="list_view(x_margin=5,spacing=10)">

      <list_item style="even_clickable" name="gif" text="Test Gif" /> 
      <list_item style="odd_clickable" name="image" text="Test Image" /> 
      <list_item style="even_clickable" name="self_layout" text="Test Self Layout" /> 
      <list_item style="odd_clickable" name="children_layout" text="Test Children Layout" /> 
      <list_item style="even_clickable" name="focus_list_view" text="Test List View Focus" /> 
      <list_item style="odd_clickable" name="auto_adjust_size_list_view" text="Test List View Auto Resize" /> 
      <list_item style="even_clickable" name="edit" text="Test Edit" /> 
      <list_item style="odd_clickable" name="multi_dialogs" text="Test Multi Dialogs" /> 
      <list_item style="even_clickable" name="switch" text="Test Switch" /> 
      <list_item style="odd_clickable" name="focus" text="Test Focus" /> 
      <list_item style="even_clickable" name="navigator1" text="Test Navigator" /> 
      <list_item style="odd_clickable" name="fullscreen" text="Test Fullscreen" /> 
      <list_item style="even_clickable" name="auto_adjust_size" text="Test Auto Adjust Resize" /> 
      <list_item style="odd_clickable" name="auto_adjust_size_max_w" text="Test Auto Adjust Resize Max W" /> 
      <list_item style="even_clickable" name="auto_adjust_size_margin" text="Test Auto Adjust Resize Margin" /> 
      <list_item style="odd_clickable" name="rotation" text="Test Rotation" /> 
      <list_item style="even_clickable" name="dialogs" text="Test Dialogs" /> 
      <list_item style="odd_clickable" name="combo_box" text="Test ComboBox" /> 
      <list_item style="even_clickable" name="button" text="Test Buttons" /> 
      <list_item style="odd_clickable" name="check_button" text="Test CheckButton" /> 
      <list_item style="even_clickable" name="popup" text="Test Popup" /> 
      <list_item style="odd_clickable" name="auto_scale_children" text="Test Auto Scale Children" /> 
      <list_item style="even_clickable" name="timer" text="Test Timer" /> 
      <list_item style="odd_clickable" name="closable" text="Test Closable" /> 
    </scroll_view>
    <scroll_bar_d name="bar" x="right" y="0" w="12" h="100%" value="0"/>
  </list_view>
  <button name="close" x="center" y="bottom:10" w="50%" h="30" text="close" on:click="quit()"/>
</window>
